{
  "version": 1,
  "projects": {
    "apis-user": {
      "root": "apps/apis/user",
      "sourceRoot": "apps/apis/user/src",
      "projectType": "application",
      "prefix": "apis-user",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/apis/user",
            "main": "apps/apis/user/src/main.ts",
            "tsConfig": "apps/apis/user/tsconfig.app.json",
            "assets": [
              "apps/apis/user/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/apis/user/src/environments/environment.ts",
                  "with": "apps/apis/user/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "apis-user:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/apis/user/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/apis/user/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hubs-main": {
      "root": "apps/hubs/main",
      "sourceRoot": "apps/hubs/main",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/hubs/main",
            "outputPath": "dist/apps/hubs/main"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve-with-services": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn start apis-user"
              },
              {
                "command": "yarn start hubs-main"
              }
            ],
            "parallel": true
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "hubs-main:build",
            "dev": true,
            "proxyConfig": "apps/hubs/main/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "hubs-main:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "hubs-main:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hubs/main/**/*.{ts,tsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/hubs/main/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hubs/main-e2e": {
      "root": "apps/hubs/main-e2e",
      "sourceRoot": "apps/hubs/main-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hubs/main-e2e/cypress.json",
            "tsConfig": "apps/hubs/main-e2e/tsconfig.e2e.json",
            "devServerTarget": "hubs-main:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hubs-main:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hubs/main-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
